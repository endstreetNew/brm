@* <CascadingAuthenticationState> *@
    <Router AppAssembly="@typeof(Program).Assembly">
        <Found Context="routeData">
            <AuthorizeRouteView RouteData="@routeData" DefaultLayout="@typeof(BrmLayout)" />
        </Found>
        <NotFound>
            <LayoutView Layout="@typeof(BrmLayout)">
                <p>Sorry, there's nothing at this address.</p>
            </LayoutView>
        </NotFound>
    </Router>
@* </CascadingAuthenticationState> *@