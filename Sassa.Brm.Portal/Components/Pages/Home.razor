@page "/"
@inject MenuItems endPoints

<div class="wrapper">
    @foreach(var item in endPoints)
    {
        <div>
            <a class="btn btn-lg btn-outline-secondary" href="@item.Url" target="_blank" style="width:350px;"><span ><img src="@item.Icon" style="width:50px;" /></span>@item.Name</a>
        </div>
    }
</div>
